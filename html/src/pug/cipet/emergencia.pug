block body
    body.cipet.inner
    
block header
    include partials/header
            
block content
            
    //ficha
    .section-row.row-inner.row-common(data-midnight="white")
        .container                  
            img(src="../images/inner/im_emergency.png", style="max-width:647px;")
                      
            h2 Material Retroreflectivo - Calcos
                
    .section-row.row-inner.inner-common
        .container
            .row
                .col-xs-12
                
                    h3 Calcomanía identificatoria <br> de Material Retro-reflectivo
                    
